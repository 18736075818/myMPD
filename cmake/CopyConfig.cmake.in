IF (NOT EXISTS "$ENV{DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@@SUBDIR@/mympd.conf")
  MESSAGE("-- Installing: $ENV{DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@@SUBDIR@/mympd.conf")
  EXECUTE_PROCESS (COMMAND @CMAKE_COMMAND@ -E copy "@CMAKE_CURRENT_BINARY_DIR@/contrib/mympd.conf" "$ENV{DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@@SUBDIR@/mympd.conf")
ELSE()
  MESSAGE("-- Installing: $ENV{DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@@SUBDIR@/mympd.conf.dist")
  EXECUTE_PROCESS (COMMAND @CMAKE_COMMAND@ -E copy "@CMAKE_CURRENT_BINARY_DIR@/contrib/mympd.conf" "$ENV{DESTDIR}@CMAKE_INSTALL_FULL_SYSCONFDIR@@SUBDIR@/mympd.conf.dist")
ENDIF ()
